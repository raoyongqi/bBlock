{
    "manifest_version": 2,
    "name": "URL Blocker",
    "version": "1.0",
    "hidden": true,
    "description": "Block access to all URLs except the specified ones.",
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "<all_urls>",
      "privacy",
      "storage"

    ],
    "background": {
      "scripts": ["background.js"]
    },
    "applications": {
      "gecko": {
        "id": "addons-my-new-addon@mozilla.com",
        "strict_min_version": "57.0"
      }
    }
  }
  